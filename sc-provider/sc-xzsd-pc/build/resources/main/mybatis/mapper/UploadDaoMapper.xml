<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzsd.pc.upload.UploadDao">
    <!--上传图片-->
    <insert id="savePhoto" parameterType="java.lang.String">
        insert into
        t_upload
        (photo,
        create_time)
        values (
        #{photo},
        now()
        )
    </insert>
</mapper>